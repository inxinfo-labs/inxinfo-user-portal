# Copy to .env and set values. No secrets in repo.
# All config is centralized in src/config/appConfig.js

# Required: Backend API base URL (auth, user, orders, etc.)
REACT_APP_API_URL=http://localhost:8080/api

# Optional: Request timeout in milliseconds
REACT_APP_API_TIMEOUT_MS=30000

# Optional: Enable debug logging (do not use in production)
REACT_APP_DEBUG=false

# Optional: Contact page fallback email
REACT_APP_CONTACT_EMAIL=satish.prasad@inxinfo.com

# Optional: EmailJS for contact form
REACT_APP_EMAILJS_SERVICE_ID=
REACT_APP_EMAILJS_TEMPLATE_ID=
REACT_APP_EMAILJS_PUBLIC_KEY=
